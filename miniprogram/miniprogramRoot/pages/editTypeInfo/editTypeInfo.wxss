/* pages/editTypeInfo/editTypeInfo.wxss */
.add_wrap{
    color: #F899F5;
}
.add_wrap .add_title_wrap{
    background:  url("data:image/png;base64,") no-repeat center;
    margin: 50rpx auto 0rpx auto;
    text-align: center;
    font-size: 50rpx;
}
.add_wrap .add_content{
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    width: 90vw;
    margin: 15rpx auto 30rpx auto;
}
.add_wrap .add_content view{
    margin-top: 15rpx;
}
.add_wrap .item_wrap input{
    display: block;
    width: 300rpx;
    height: 45rpx;
    border: #F899F5 2rpx solid;
    border-radius: 10rpx;
    box-sizing: border-box;
    margin-top: 15rpx;
}
.add_wrap .uploder_img_wrap{
    width: 90vw;
    margin: auto;
}
.add_wrap .uploder_img_wrap .uploder_img{
    display: flex;
    flex-wrap: wrap;
    margin-top: 15rpx;
}
.uploder_img_wrap .uploder_img .show_img,
.uploder_img_wrap .uploder_img .show_img image{
    width: 200rpx;
    height: 200rpx;
}
.uploder_img .show_img .deleta_button{
    position: absolute;
    z-index: 10;
    top: -8rpx;
    right: -8rpx;
    width: 25rpx;
    height: 25rpx;
    background: url("data:image/png;base64,") no-repeat center;
}
.uploder_img_wrap .uploder_img .show_img{
    position: relative;
    margin: 15rpx 12rpx 0rpx  12rpx;
}
.add_wrap .uploder_img_wrap .add_img_button{
    margin: 15rpx 12rpx 0rpx  12rpx;
    width: 200rpx;
    height: 200rpx;
    background: url("data:image/png;base64,") no-repeat center;
}
.add_wrap .add_computer_info{
    margin: 50rpx auto 50rpx auto;
    width: 400rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    color: #FFF;
    background-color: #F899F5;
    border-radius: 50rpx;
}