/* pages/index/index.wxss */
view{
    padding: 0rpx;
}
.top_wrap{
    width: 100vw;
    height: 400rpx;
    font-size: 35rpx;
    color: #FFF;
    background-color: #F899F5;
    text-align: center;
    border: 1px transparent solid;
    box-sizing: border-box;
    line-height: 80rpx;
}
.top_wrap:first-child{
    padding-top: 100rpx;
    margin-bottom: 20rpx;
}
.top_wrap .top_line{
    height: 6rpx;
    background-color: #FFF;
    width: 85vw;
    margin: auto;
}
.top_wrap .top_english{
    font-size: 25rpx;
}
.search_wrap{
    display: flex;
    justify-content: center;
    align-items: center;
    width: 95vw;
    box-sizing: border-box;
    margin: auto;
}
.search_wrap .found_by_id,
.search_wrap .found_by_name{
    width: 120rpx;
    height: 80rpx;
    border: 2rpx #F899F5 solid;
    font-size: 20rpx;
    box-sizing: border-box;
    line-height: 80rpx;
    text-align: center;
}
.search_wrap .found_by_id{
    border-radius: 25rpx 0rpx 0rpx 25rpx;
}
.search_wrap .found_by_name{
    border-radius: 0rpx 25rpx 25rpx 0rpx;
}
.found_active{
    background-color: #F899F5;
    color: #FFF;
}
.search_body_wrap{
    display: flex;
    justify-content: center;
    align-items: center;
    flex: 1;
    height: 80rpx;
    box-sizing: border-box;
    border: 2rpx #F899F5 solid;
    margin-left: 25rpx;
    border-radius: 25rpx;
    padding-left: 15rpx;
}
.search_wrap .search_body_wrap image{
    width: 26rpx;
    height: 28rpx;
}
.search_body_wrap input{
    flex: 1;
    padding-left: 15rpx;
}
.search_body_wrap .search_button{
    width: 120rpx;
    height: 80rpx;
    border-radius: 0rpx 25rpx 25rpx 0rpx;
    background-color: #F899F5;
    color: #FFF;
    text-align: center;
    line-height: 80rpx;
    margin-right: -2rpx;
}
.add_wrap{
    color: #F899F5;
}
.add_wrap .add_title_wrap{
    background:  url("data:image/png;base64,") no-repeat center;
    margin: 50rpx auto 0rpx auto;
    text-align: center;
    font-size: 50rpx;
}
.add_wrap .add_content{
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    width: 90vw;
    margin: 15rpx auto 30rpx auto;
}
.add_wrap .add_content view{
    margin-top: 15rpx;
}
.add_wrap .item_wrap input{
    display: block;
    width: 300rpx;
    height: 45rpx;
    border: #F899F5 2rpx solid;
    border-radius: 10rpx;
    box-sizing: border-box;
    margin-top: 15rpx;
}
.add_wrap .uploder_img_wrap{
    width: 90vw;
    margin: auto;
}
.add_wrap .uploder_img_wrap .uploder_img{
    display: flex;
    flex-wrap: wrap;
    margin-top: 15rpx;
}
.uploder_img_wrap .uploder_img .show_img,
.uploder_img_wrap .uploder_img .show_img image{
    width: 200rpx;
    height: 200rpx;
}
.uploder_img .show_img .deleta_button{
    position: absolute;
    z-index: 10;
    top: -8rpx;
    right: -8rpx;
    width: 25rpx;
    height: 25rpx;
    background: url("data:image/png;base64,") no-repeat center;
}
.uploder_img_wrap .uploder_img .show_img{
    position: relative;
    margin: 15rpx 12rpx 0rpx  12rpx;
}
.add_wrap .uploder_img_wrap .add_img_button{
    margin: 15rpx 12rpx 0rpx  12rpx;
    width: 200rpx;
    height: 200rpx;
    background: url("data:image/png;base64,") no-repeat center;
}
.add_wrap .add_computer_info{
    margin: 50rpx auto 50rpx auto;
    width: 400rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    color: #FFF;
    background-color: #F899F5;
    border-radius: 50rpx;
}